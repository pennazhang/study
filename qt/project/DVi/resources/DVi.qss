#m_pPresetFrame {
	background-color: #282828; 
}

#m_pDSPFrame {
	background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #c3c3c3, stop:0.4 #ffffff, stop:1 #626262);
	border: 5px solid #000000;
}

#m_pReminderFrame {
	background-color: #282828; 
}

#m_pCrownLabel {
	image: url(:/reminder.png);
}

#m_pPresetName, #m_pReminderLabel {
	border: 1px solid #ffffff;
	color: #ffffff;
	font: 12px;
	padding-left: 2px; 
}


#m_pSignalGenButton, #m_pRecallButton, #m_pStoreButton, #m_pAmpButton {
	border: 1px solid #e0e0e0; 
	color: #e0e0e0;
	background-color:qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #a0a0a0, stop:0.55 #606060, stop:0.6 #404040, stop:1 #101010);
	font: 12px;
	font: bold;
}

#m_pSignalGenButton:hover, #m_pRecallButton:hover, #m_pStoreButton:hover, #m_pAmpButton:hover {
	color: #ffffff;
	background-color:qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #d0d0d0, stop:0.55 #606060, stop:0.6 #404040, stop:1 #101010);
	border: 1px solid #ffffff; 
}

