#m_pPanelWidget {
	border: 1px solid #000000;
	background-color: #5a5a5a;
}

#m_pFrame1, #m_pFrame2, #m_pFrame3 {
   background-color: #000000;
   border-right: 1px solid #ffffff;
}

#m_pMenuBar {
	background-color: transparent; 
	spacing: 21px;
	font: 12px;
	color: #ff0000;
}

#m_pMenuBar::item {
	color: #d0d0d0;
	background: transparent;
 }

#m_pMenuBar::item:selected { 
	color: #ffffff;
    background: transparent;
}

#m_pConnectButton:hover { 
	color: #FFFFFF; 
}  

#m_pConnectButton:pressed {	
	color: #FFFFFF;     
	padding-left: 2px;     
	padding-top: 2px; 
} 
				

#m_pConnectButton[markFlag=true]:enabled { 
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, 
				stop: 0 #ffffff, stop: 0.05 #ffffff, stop: 0.1 #a6bad9, 
				stop: 0.15 #7597c4,
				stop: 0.25 #007acc, stop: 0.85 #007acc, 
				stop: 0.90 #7597c4, stop: 0.95 #ffffff, stop: 1 #ffffff); 
	border: 1px solid #404040; 
	font: 12px; 
	border-radius: 2; 
	color: #d0d0d0; 
} 

#m_pConnectButton[markFlag=false]:enabled { 
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, 
				stop: 0 #ffffff, stop: 0.05 #e80000, 
				stop: 0.1 #ab0000, stop: 0.15 #880000,  
				stop: 0.2 #600000, stop: 0.9 #3f0000, 
				stop: 0.95 #b80000, stop: 1 #ffffff); 
	border: 1px solid #404040; 
	border-radius: 2; 
	font: 12px; 
	color: #d0d0d0; 
} 



#ButtonVerticalSlider::groove:vertical {
	border: 1px solid transparent; 
}

#ButtonVerticalSlider::handle {
	 image: url(:/QSlider-button.png);
 }

#ButtonVerticalSlider::handle:focus {
	 image: url(:/QSlider-button-selected.png); 
 }

#ButtonHorizontalSlider {
	qproperty-textColor: #c0c0c0;
}

#ButtonHorizontalSlider::groove:horizontal {
	border: 1px solid transparent; 
	height: 25;
}

#ButtonHorizontalSlider::handle {
	 image: url(:/QSlider-horizontal-button.png);
 }

#ButtonHorizontalSlider::handle:focus {
	 image: url(:/QSlider-horizontal-button-selected.png); 
 }
